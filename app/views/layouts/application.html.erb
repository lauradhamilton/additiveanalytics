  <body>

    <!-- Fixed navbar -->
    <div class="navbar navbar-default navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/">Additive<font style="color: #C83535;">Analytics</font></a>
        </div>
        <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav">
            <li><a href="/">Home</a></li>
            <li><a href="/solutions">Solutions</a></li>
            <li><a href="/about">About</a></li>
            <li><a href="/contact">Contact</a></li>
            <li><a data-no-turbolink="true" href="/blog">Blog</a></li>
            <%= render 'devise/menu/login_items' %>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </div>

    <div class="container">
      <p class="notice"><%= notice %></p>
      <p class="alert"><%= alert %></p>

<%= yield %>

    <!-- FOOTER -->
    <footer>
      <p class="pull-right"><a href="/">Back to top</a></p>
      <p>&copy; 2013-<%=Time.now.year %> Additive Analytics, LLC. All rights reserved &middot; <a href="/privacy_policy">Privacy</a> &middot; <a href="/terms">Terms</a></p>
    </footer>
  </div><!-- /.container -->


    <!-- Bootstrap core JavaScript
